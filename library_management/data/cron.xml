
<odoo>
    <data noupdate="1">
        <record id="ir_cron_membership_expire" model="ir.cron">
            <field name="name">Membership: Expire Memberships</field>
            <field name="model_id" ref="model_library_membership"/>
            <field name="state">code</field>
            <field name="code">model.search([('end_date', '&lt;', fields.Date.today()), ('state', '=', 'active')]).write({'state': 'expired'})</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
        </record>
    </data>
</odoo>
